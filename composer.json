{
    "name": "elcodi/bamboo",
    "license": "MIT",
    "type": "project",
    "description": "Bamboo, an ecommerce suite built with Elcodi",
    "authors": [
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        },
        {
            "name": "Aldo Chiecchia",
            "email": "aldo.chiecchia@befactory.es"
        },
        {
            "name": "The Awesome Elcodi Community",
            "homepage": "https://github.com/elcodi/bamboo/contributors"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.9.1",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.9.1.js",
                    "type": "file"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-openssl": "*",

        "symfony/symfony": "~2.6",
        "symfony/assetic-bundle": "~2.5",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.6",
        "symfony/proxy-manager-bridge": "~2.6, >=2.6.3",

        "ocramius/proxy-manager": "~1.0",
        "doctrine/orm": "~2.4",
        "doctrine/doctrine-bundle": "~1.2",

        "twig/extensions": "~1.0",
        "doctrine/doctrine-fixtures-bundle": "v2.2.0",
        "sensio/distribution-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~3.0",
        "jquery/jquery": "1.9.*",
        "jms/aop-bundle": "~1.0",
        "jms/serializer-bundle": "0.13.0",
        "liip/doctrine-cache-bundle": "1.0.3",
        "jms/security-extra-bundle": "1.5.1",
        "braincrafted/bootstrap-bundle": "1.*",
        "twbs/bootstrap": "2.3.*",
        "leafo/lessphp": "0.3.9",

        "amazonwebservices/aws-sdk-for-php": "1.6.2",

        "mmoreram/controller-extra-bundle": "~1.0",
        "ornicar/gravatar-bundle" : "1.1.2",
        "knplabs/knp-gaufrette-bundle": "~0.1.0",
        "paymentsuite/paymentsuite": "~1.4.0",
        "paymentsuite/paypal-web-checkout-bundle": "~1.4.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "hwi/oauth-bundle": "0.4.*@dev",
        "snc/redis-bundle": "~1.1",

        "elcodi/bamboo-bundle": "~0.4.0",
        "elcodi/attribute-bundle": "~0.4.0",
        "elcodi/banner-bundle": "~0.4.0",
        "elcodi/cart-bundle": "~0.4.0",
        "elcodi/cart-coupon-bundle": "~0.4.0",
        "elcodi/core-bundle": "~0.4.0",
        "elcodi/coupon-bundle": "~0.4.0",
        "elcodi/currency-bundle": "~0.4.0",
        "elcodi/geo-bundle": "~0.4.0",
        "elcodi/language-bundle": "~0.4.0",
        "elcodi/media-bundle": "~0.4.0",
        "elcodi/menu-bundle": "~0.4.0",
        "elcodi/newsletter-bundle": "~0.4.0",
        "elcodi/product-bundle": "~0.4.0",
        "elcodi/referral-program-bundle": "~0.4.0",
        "elcodi/rule-bundle": "~0.4.0",
        "elcodi/user-bundle": "~0.4.0",
        "elcodi/tax-bundle": "~0.4.0",
        "elcodi/state-transition-machine-bundle": "~0.4.0",
        "elcodi/entity-translator-bundle": "~0.4.0",
        "elcodi/page-bundle": "~0.4.0",
        "elcodi/configuration-bundle": "~0.4.0",
        "elcodi/metric-bundle": "~0.4.0",
        "elcodi/sitemap-bundle": "~0.4.0",

        "elcodi-templates/template": "~0.4.0",
        "elcodi-templates/store-template-bundle": "~0.4.0",
        "elcodi-templates/store-white-template-bundle": "~0.4.0"
    },
    "require-dev": {
        "elcodi/elcodi": "~0.4.18",
        "mmoreram/php-formatter": "~1.0",
        "behat/symfony2-extension": "~2.0@dev",
        "behat/mink-extension": "~2.0@dev",
        "behat/mink-browserkit-driver": "~1.1@dev"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "autoload": {
        "psr-4": {
            "Elcodi\\": [
                "src/Elcodi"
            ]
        },
        "files": ["app/AppKernel.php"]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
